{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "prettier-plugin-wromo",
            "skipFiles": [
                "{fileExtname}/.wromo",
                "{fileExtname}/.wromo/**",
                "{fileExtname}/.wromo/**/*",
                "{fileExtname}/.wromo/**/*.{wromo,js,ts}",
                "{fileExtname}/.wromo/**/*.{wromo,js,ts}/**",
                "{fileExtname}/.wromo/**/*.{wromo,js,ts}/**/*",
                "{fileExtname}/.wromo/**/*.{wromo,js,ts}/**/*.{wromo,js,ts}",
                "{fileExtname}/.wromo/**/*.{wromo,js,ts}/**/*.{wromo,js,ts}/**",
                "{fileExtname}/.wromo/**/*.{wromo,js,ts}/**/*.{wromo,js,ts}/**/*",
                "{fileExtname}/.wromo/**/*.{wromo,js,ts}/**/*.{wromo,js,ts}/**/*.{wromo,js,ts}"
            ],
           
            "program": "${file}"
        }
    ]
}

